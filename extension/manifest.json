{
  "manifest_version": 3,
  "name": "Clickbait",
  "version": "1.0",
  "description": "Tagging spam or misleading videos with community feedback",
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["inject.js"],
      "css": ["inject.css"],
      "run_at": "document_start"
    }
  ]
}
